    @Scripts.Render("~/bundles/extjs_Layout")

</head>
<body style="background-color:White; margin:0;overflow:hidden" scroll="no">

    <div id="body">
        @RenderSection("featured", required: false)
        @* <section class="content-wrapper main-content clear-fix">
                @RenderBody()
