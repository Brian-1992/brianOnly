                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>                        SELECT
                        B.AGEN_NO,      
                        C.AGEN_NAMEC, 
                        A.INVOICE,      
                        C.UNI_NO,       
                        SUM(A.PO_QTY * ( A.PO_PRICE - A.DISC_CPRICE)) MONEY_1,  
                        SUM(A.PO_QTY * A.DISC_CPRICE) MONEY_2,                  
                        SUM(A.PO_QTY * A.PO_PRICE ) MONEY_3                  
                        FROM PH_INVOICE A, MM_PO_M B, PH_VENDER C
                        WHERE A.PO_NO = B.PO_NO
                            AND B.AGEN_NO = C.AGEN_NO
 GROUP BY TWN_YYYMM(A.INVOICE_DT), B.AGEN_NO, C.AGEN_NAMEC, A.INVOICE, C.UNI_NO</CommandText>
