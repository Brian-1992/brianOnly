@*@{
    Layout = null;
}*@
@{
    ViewBag.Title = "";
}
@section scripts
{
    @{ var s = string.Format("~/{0}", ViewBag.Script); }
    <script type="text/javascript">
        callParDeltaFlow();
    Ext.onReady(function () {
        var url = '@Url.Content(s)';
        var tabs = Ext.widget('tabpanel', {
            plain: true,
            header: false,
            resizeTabs: true,
            defaults: {
                autoScroll: false
            },
            items: [{
                id: 'tab1',
                header: false,
                title:'',
                html: '<iframe src=' + url + ' id="mainContent" width="100%" height="100%" frameborder="0"></iframe>'
            }]
        });
        Ext.getCmp('tabbar-1010').hidden = true;
        var viewport = Ext.create('Ext.Viewport', {
            layout: {
                type: 'border',
                padding: 0
            },
            defaults: {
                split: true
            },
            items: [{
                layout: {
                    type: 'border',
                    padding: 0
                },
                region: 'center',
                border: false,
                items: [{
                    itemId: 'center-panel',
                    region: 'center',
                    layout: 'fit',
                    border: false,
                    title: '',
                    items: [tabs]
                }
                ]

            }]
        });
    });


</script>
}
